   <xs:element name="updates">
      <xs:annotation>
         <xs:documentation>Contains information about significant updates to the documentation.</xs:documentation>
      </xs:annotation>      
      <xs:complexType mixed="false">
         <xs:sequence minOccurs="0" maxOccurs="1">
            <xs:element ref="update" maxOccurs="unbounded"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   
   <xs:element name="update">
      <xs:annotation>
         <xs:documentation>Contains information about a significant update to the documentation.</xs:documentation>
      </xs:annotation>      
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="title" minOccurs="1">
               <xs:annotation>
                  <xs:documentation>Required. Title/heading of this change.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element ref="ulink" minOccurs="0">
               <xs:annotation>
                  <xs:documentation>Optional. Specifies the link to the page for this update. If excluded, it will be the URL of the page.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element ref="para" minOccurs="1">
               <xs:annotation>
                  <xs:documentation>Required. Contains the synopsis of the change.</xs:documentation>
               </xs:annotation>               
            </xs:element>
            <xs:element name="details" minOccurs="0" maxOccurs="1">
               <xs:complexType>
                  <xs:choice minOccurs="0" maxOccurs="unbounded">
                     <xs:group ref="para.blocks"/>
                     <xs:group ref="list.blocks"/>
                     <xs:group ref="admonition.blocks"/>
                  </xs:choice>
               </xs:complexType>
            </xs:element>
         </xs:sequence>
         <xs:attribute name="date" type="xs:date">
            <xs:annotation>
               <xs:documentation>Required. Specifies the date the change was made.</xs:documentation>
            </xs:annotation>            
         </xs:attribute>
         <xs:attributeGroup ref="profiling.attributes"/>
         <xs:attributeGroup ref="effectivity.attributes"/>
      </xs:complexType>
   </xs:element>
   
