                <thead>
                    <row>
                        <entry>Dimension</entry>
                        <entry>Description</entry>
                    </row>
                </thead>
                <tbody>
                    <row>
                        <entry>
                            <code>Fleet</code>
                        </entry>
                        <entry>
                            <para>The name of the fleet.</para>
                        </entry>
                    </row>
                </tbody>
            </tgroup>
        </table>
    </section>'>

